<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ZPXX">
<select id="ZPXXDH" parameterType="java.util.Map" resultType="java.util.Map">
	select ZPXX01 , ZPXX02 ZPMC
	from ZPXX  
	<if test="GSXX01 != null and GSXX01 != ''">
      where  GSXX01 = #{GSXX01}
       </if>
    <if test="ZPXX01 != null and ZPXX01 != ''">
      AND  ZPXX01 LIKE '%'||#{ZPXX01}||'%'
       </if>
    <if test="ZPXX02 != null and ZPXX02 != ''">
      AND  ZPXX02 LIKE '%'||#{ZPXX02}||'%'
     </if>
</select>
<select id="ZPXXXL" parameterType="java.util.Map" resultType="java.util.Map">
	select ZPXX01 KEY, ZPXX02 VALUE
	from ZPXX  
	<if test="GSXX01 != null and GSXX01 != ''">
      where  GSXX01 = #{GSXX01}
       </if>
    <if test="ZPXX01 != null and ZPXX01 != ''">
      AND  ZPXX01 LIKE '%'||#{ZPXX01}||'%'
       </if>
    <if test="ZPXX02 != null and ZPXX02 != ''">
      AND  ZPXX02 LIKE '%'||#{ZPXX02}||'%'
     </if>
</select>
</mapper>