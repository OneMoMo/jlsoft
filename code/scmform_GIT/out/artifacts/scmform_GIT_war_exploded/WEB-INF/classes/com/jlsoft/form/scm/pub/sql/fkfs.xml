<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FKFS">
	<select id="FKFS" parameterType="Map" resultType="java.util.Map">
	 SELECT FKFS01, FKFS02 FKFSMC, FKFS01 key, FKFS02 value
