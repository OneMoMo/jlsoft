<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MONGO_JK1">
	<!-- 查询ORACEL定义表KCSLZT -->
  <select id="SCM_JK" parameterType="java.util.Map" resultType="java.util.Map">
 	select A.JKLX01,A.JKLX02
	  from JKLX A 
 	 where 1 = 1
 	<if test="JKLX != null and JKLX != ''">
		AND (A.JKLX01 = #{JKLX}  OR A.JKLX01 LIKE '%'|| #{JKLX} ||'%')
	</if>
	</select>
 
</mapper>