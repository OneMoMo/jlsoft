<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mdsbbx">
	<select id="FYLX" parameterType="Map" resultType="java.util.Map">
select FYLX01,FYLX02 from fylx A where 1=1
       <if test="GSXX01 != null and GSXX01 != ''">
       AND A.GSXX01 = #{GSXX01}
       </if>
       <if test="FYLX02 != null and GSXX01 != ''">
       AND A.FYLX02 = #{FYXM}
       </if>
	</select>
	
	<select id="BZCX" parameterType="Map" resultType="java.util.Map">
select WBBZ02 from WBBZ
 A where 1=1
	</select>
	
	<select id="BMCX" parameterType="Map" resultType="java.util.Map">
select FYBM02 from fybm WHERE 1=1 
	</select>
</mapper>
